<?xml version="1.0" encoding="UTF-8"?>
<arquillian xmlns="http://jboss.org/schema/arquillian"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://jboss.org/schema/arquillian
    http://jboss.org/schema/arquillian/arquillian_1_0.xsd">

    <group qualifier="containers" default="true">
        <container qualifier="jbossas-managed" default="true">
            <configuration>
                <property name="jbossHome">/home/smikloso/devel/servers/jboss-as-7.1.1.Final</property>
            </configuration>
        </container>
        <container qualifier="android-managed">
            <configuration>
                <property name="avdName">containerTest</property>
                <property name="sdSize">128M</property>
                <property name="emulatorBootupTimeoutInSeconds">600</property>
            </configuration>
        </container>
    </group>
</arquillian>
